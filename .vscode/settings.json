{
    "Lua.workspace.library": [
        "${addons}/love2d/module/library"
    ],
    "Lua.runtime.version": "LuaJIT",
    "Lua.runtime.special": {
        "love.filesystem.load": "loadfile"
    },
    "Lua.workspace.checkThirdParty": false,
    "Lua.diagnostics.disable": [
        "trailing-space",
        "undefined-field",
        "lowercase-global",
        "undefined-global",
        "cast-local-type",
        "param-type-mismatch",
        "redundant-parameter",
        "deprecated",
        "missing-fields",
        "undefined-doc-name",
        "undefined-doc-param",
        "inject-field",
        "missing-parameter",
        "need-check-nil"
    ],
    "Lua.completion.autoRequire": false
}